{
    left
    {
        type            fixedFluxPressure;
        gradient        nonuniform 
0
;
        value           nonuniform 
0
;
    }
    right
    {
        type            totalPressure;
        rho             rho;
        psi             none;
        gamma           1;
        p0              nonuniform 
0
;
        value           nonuniform 
0
;
    }
    bottom
    {
        type            fixedFluxPressure;
        gradient        nonuniform 
0
;
        value           nonuniform 
0
;
    }
    top
    {
        type            fixedFluxPressure;
        gradient        uniform 0;
        value           uniform 100000;
    }
    frontAndBack
    {
        type            empty;
    }
    procBoundary4to1
    {
        type            processor;
        value           uniform 100000;
    }
    procBoundary4to3
    {
        type            processor;
        value           uniform 100000;
