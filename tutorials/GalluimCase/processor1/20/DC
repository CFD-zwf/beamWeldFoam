FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "20";
    object      DC;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 -1 0 0 0 0];

internalField   uniform 1e+18;

boundaryField
{
    left
    {
        type            calculated;
        value           nonuniform 
0
;
    }
    right
    {
        type            calculated;
        value           nonuniform 
0
;
    }
    bottom
    {
        type            calculated;
        value           uniform 1e+18;
    }
    top
    {
        type            calculated;
        value           nonuniform 
0
;
    }
    frontAndBack
    {
        type            empty;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform 1e+18;
    }
    procBoundary1to2
    {
        type            processor;
        value           uniform 1e+18;
    }
    procBoundary1to4
    {
        type            processor;
        value           uniform 1e+18;
    }
}


