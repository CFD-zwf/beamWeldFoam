FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.phase1 0
);

regions
(



    boxToCell
    {
        box (-0.085 0.02 -0.05) (0.085 0.06 0.05);
        fieldValues
        (
    volScalarFieldValue alpha.phase1 1
        );
    }



);


