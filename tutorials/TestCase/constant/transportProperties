}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases (phase1 phase2);

phase1
{
    transportModel  Newtonian;
    nu              1e-06;
    rho             7874;
    cp  100.0;//500;
    kappa  80.0; 
    Tsolidus    1920;
    Tliquidus   1970;
    LatentHeat  500.0;//270000;
    beta    1.0e-5;

    Tvap 3000.0;
    Mm 0.05;
LatentHeatVap 600000.0;

}

